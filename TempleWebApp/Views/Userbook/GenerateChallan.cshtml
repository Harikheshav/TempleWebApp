@using Microsoft.AspNetCore.Http
@inject IHttpContextAccessor HttpContextAccessor
@{
ViewBag.uid =   @HttpContextAccessor.HttpContext.Session.GetString("UserId");
}
@{
    ViewData["Title"] = "Detail";
}
@if (ViewBag.uid != null)
{
<div>
    <h4>Bank Challan</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Booking Type
        </dt>
        <dd class = "col-sm-10">
            @TempData["Booking_Type"]
        </dd>
        <dt class = "col-sm-2">
            Booking Details
        </dt>
        <dd class = "col-sm-10">
            @ViewBag.Details
        </dd>
        <dt class = "col-sm-2">
            Start Date
        </dt>
        <dd class = "col-sm-10">
            @ViewBag.StartDate
        </dd>
        <dt class = "col-sm-2">
            End Date
        </dt>
        <dd class = "col-sm-10">
            @ViewBag.EndDate
        </dd>
        <dt class = "col-sm-2">
               Cost
        </dt>
        <dd class = "col-sm-10">
            @ViewBag.Cost
        </dd>
        <dt class = "col-sm-2">
            User Booked
        </dt>
        <dd class = "col-sm-10">
            @ViewBag.User
        </dd>
    </dl>
    <div><button onclick="window.print()">Save</button></div>
</div>
}
else
{
<b>Either you are logged in as a Admin or you are logged out...</b>
}